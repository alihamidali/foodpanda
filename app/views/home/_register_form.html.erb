<div class="form-group">
  <%= f.text_field :first_name, :class => 'form-control', :id => "first_name", 'aria-describedby' => "emailHelp", 'placeholder' => "First Name" %>
</div>
<div class="form-group">
  <%= f.text_field :last_name, :class => "form-control", :id => "exampleInputPassword1", 'placeholder' => "Last Name" %>
</div>
<div class="form-group">
  <%= f.email_field :email, :class => "form-control", :id => "exampleInputPassword1", 'placeholder' => "Email" %>
</div>
<div class="form-group">
  <%= f.text_field :phone_no, :class => "form-control", :id => "exampleInputPassword1", 'placeholder' => "Phone no" %>
</div>
<div class="form-group">
  <%= f.password_field :password, :class => "form-control", :id => "exampleInputPassword1", 'placeholder' => "Password" %>
</div>